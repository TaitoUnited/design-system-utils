{
  "name": "@taito/design-system-utils",
  "version": "1.0.0",
  "description": "Set of utility functions for transforming design tokens into theme compatible format.",
  "scripts": {
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./native": {
      "require": "./dist/native.js",
      "import": "./dist/native.mjs"
    },
    "./web": {
      "require": "./dist/web.js",
      "import": "./dist/web.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaitoUnited/design-system-utils.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Taito United Oy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TaitoUnited/design-system-utils/issues"
  },
  "homepage": "https://github.com/TaitoUnited/design-system-utils#readme",
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/native.ts",
      "src/web.ts"
    ],
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
